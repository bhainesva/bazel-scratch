{
  "exclude": [],
  "compilerOptions": {
    "target": "es2016",                           
    "module": "ESNext",                   
    "paths": {
      "/*": ["./*"],
      "@gen/*": [
        "./*",
        "./bazel-bin/*"
      ],
    },                                  
    "allowJs": true,                       
    "declaration": true,                          
    "esModuleInterop": true,                        
    "forceConsistentCasingInFileNames": true,        
    "strict": true,                                   
    "skipLibCheck": true                              
  }
}
